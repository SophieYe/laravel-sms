    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^5.5|^7.24|^8.0",
        "alibabacloud/client": "~1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "authors": [
        {
            "name": "hinet",
            "email": "63603636@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
